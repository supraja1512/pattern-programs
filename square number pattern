///8.NUMBER SERIES8////
#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int n,s;
    cin>>n;
    s=(n+1)/2;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            if((j==s)||(i==s))
            {
                cout<<"0";
            }
            else
            {
                cout<<"1";
            }
        }
        cout<<endl;
    }
}
