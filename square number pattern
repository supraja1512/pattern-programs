///2./NUMBER SERIES2////

#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            if(i%2==0)
            {
                cout<<"0";
            }
            else
            {
            cout<<"1";
            }
        }
        cout<<endl;
    }
}
