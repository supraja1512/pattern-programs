////5.NUMBER SERIES5////
#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    double n;
    double s;
    cin>>n;
    s=ceil(n/2);
   
    for(int i=1;i<=n;i++)
    {
       for(int j=1;j<=n;j++)
        {
            
           if((i==j)&&(j==s)&&(s==i))
            {
                cout<<"0";
           }
            else
            {
                cout<<"1";
            }
        }
        cout<<endl;
    }
}
