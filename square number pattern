{
    int n;
    cin>>n;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            if(j%2==0)
            {
                cout<<"1";
            }
            else
            {
            cout<<"0";
            }
        }
        cout<<endl;
    }
}
