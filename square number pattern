///6.NUMBER SERIES////
#include<iostream>
using namespace std;
int main()
{
    int n,s;
    cin>>n;
    
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            s=i+j;
            if((s%2==0))
            {
                cout<<"1";
            }
            else
            {
                cout<<"0";
            }
        }
        cout<<endl;
    }
}
