///7.NUMBER SERIES7////
#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int n,s;
    cin>>n;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            if((j==3)||(i==3))
            {
                cout<<"0";
            }
            else
            {
                cout<<"1";
            }
        }
        cout<<endl;
    }
}
